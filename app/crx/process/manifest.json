{
  "name": "Process Monitor",
  "version": "1.0",
  "description": "Extract info from ARC app by monitoring processes",
  "permissions": [
    "processes"
  ],
  "browser_action": {
    "default_title": "Process Monitor",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "icons": { "16": "icon.png", "128": "icon.png" },
  "manifest_version": 2
}
